#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../systeminformation@5.18.3/node_modules/systeminformation/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../systeminformation@5.18.3/node_modules/systeminformation/lib/cli.js" "$@"
fi
