{
  "name": "felix-api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "tsx watch ./src/index.ts | pino-pretty",
    "build": "rm -rf ./dist && tsc",
    "prepare": "husky install",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "prisma:prepare": "pnpm prisma:generate && pnpm prisma:migrate && pnpm seed:dev",
    "prisma:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "seed:dev": "tsx ./src/prisma/seed/index.ts",
    "seed:prod": "node ./dist/prisma/seed/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/got": "^9.6.12",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^4.16.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
