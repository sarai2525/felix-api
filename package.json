    "dayjs": "^1.11.8",
    "express": "^4.18.2",
    "firebase-admin": "^11.9.0",
    "got": "^13.0.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "pm2": "^5.3.0",
    "zod": "^3.21.4",
    "zod-prisma-types": "^2.7.4"
  },
  "engines": {
    "node": "19.x",
    "pnpm": "8.x",
    "npm": "Use pnpm instead of npm",
    "yarn": "Use pnpm instead of yarn"
